package com.webonise.assignment.dinningphilosopher;

public class PhilosopherDining 
{

	Chopstick chopstick[]= new Chopstick[5];

	PhilosopherDining()
	{
		for(int i=0;i<5;i++)
			chopstick[i]=new Chopstick();
		Philosopher rana=new Philosopher(1, "rana",chopstick[1],chopstick[0]);
		Philosopher anvay=new Philosopher(2, "anvay",chopstick[2],chopstick[1]);
		Philosopher vijay=new Philosopher(3, "vijay",chopstick[3],chopstick[2]);
		Philosopher ameya=new Philosopher(4, "ameya",chopstick[4],chopstick[3]);
		Philosopher bhuvan=new Philosopher(5, "bhuvan",chopstick[0],chopstick[4]);
	}
	public static void main (String args[])
	{
		PhilosopherDining init=new PhilosopherDining();
	}
}
