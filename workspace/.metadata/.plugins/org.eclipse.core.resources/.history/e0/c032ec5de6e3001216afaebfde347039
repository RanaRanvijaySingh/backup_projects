package com.webonise.assignment.collection;



public class Person {

	private int id;
	private String name;
	private String college;
	private int age;
	Person(int id,String name,String college,int age)
	{
		this.id=id;
		this.name=name;
		this.college=college;
		this.age=age;
	}
	Person(){}
	public String getName()
	{
		return this.name;
	}
	public int getId()
	{
		return this.id;
	}
	public int getAge()
	{
		return this.age;
	}
	public String getCollege()
	{
		return this.college;
	}
	public int hashCode() {
        return new HashCodeBuilder(1583,2179).append(id)
        		.append(name)
        		.append(salary).hashCode();
}

	
}
